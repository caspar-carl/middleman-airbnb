<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Map Layout</title>
  </head>
  <body>
    <div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('https://kitt.lewagon.com/placeholder/cities/berlin');">
    <div class="the__team">
      <div class="banner-content">
        <h1>Meet the team</h1>
        <p>Learn to code with us in Berlin</p>
        <a class="btn btn-primary btn-lg">Start now</a>
      </div>
      <div class="team__buddy">
        <ul class="list-inline text-center">
          <li>
            <img class="buddy" src="https://avatars3.githubusercontent.com/u/40460759?v=4" alt="">
            <h3>Henry Stahl</h3>
          </li>
          <li>
            <img class="buddy" src="https://avatars3.githubusercontent.com/u/40319710?v=4" alt="">
            <h3>Caspar Schroeder</h3>
          </li>
          <li>
            <img class="buddy" src="https://avatars1.githubusercontent.com/u/40363168?v=4" alt="">
            <h3>Joni Schroeder</h3>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <div class="map">
    <script>
      function initMap() {
      // Create a map object and specify the DOM element for display.
      var map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: 52.5206, lng: 13.4098},
      scrollwheel: false,
      zoom: 8
      });
      }
      </script>
      <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
      <!-- Latest compiled and minified JavaScript -->
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/  bootstrap.min.js"></script>
      <!-- Gmaps JavaScript -->
      <script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzER7O10ZOUp7FloJzMe2r8X1GIQhgt7Q&callback=initMap">
      </script>
    </div>
  </body>
</html>
